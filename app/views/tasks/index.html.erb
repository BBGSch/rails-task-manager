<ul>
  <% @tasks.each do |task| %>
    <li><%=link_to task.title, "/tasks/#{task.id}" %> | <%= task.details %> | <%= task.completed %> 
      <%=link_to "delete", "/tasks/#{task.id}", method: :delete %> <%=link_to "edit", "/tasks/#{task.id}/edit" %> </li>
  <% end %>
</ul>




